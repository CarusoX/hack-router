{
  "name": "facebook/hack-router",
  "description": "URI routing for Hack",
  "keywords": ["hack", "router", "routing", "hhvm"],
  "homepage": "https://github.com/hhvm/hack-router",
  "require": {
    "hack-psr/psr7-http-message-hhi": "^0.2.0|^1.0.0",
    "hhvm/hsl": "^1.0.0rc1|^1.0.0",
    "hhvm/hhvm-autoload": "^1.5.3"
  },
  "autoload": {
    "classmap": [ "src/" ]
  },
  "autoload-dev": {
    "classmap": [ "tests/" ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.1",
    "91carriage/phpunit-hhi": "^5.1",
    "zendframework/zend-diactoros": "^1.3",
    "facebook/fbexpect": "^0.3.0"
  }
}
